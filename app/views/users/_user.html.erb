<div class="card">
  <div class="card-header">
    <div class="fa fa-user"></div>
    <%= link_to user.email, user_path(user) %>
  </div>
  <div class="card-footer">
    Signed up
    <%= time_ago_in_words(user.created_at) %>
    ago
  </div>
  </div>
  <div class="card-footer">
  <% user.roles.each do |role| %>
    <div class="badge badge-info">
      <%= role.name %>
    </div>
  <% end %>
</div>